{
    "require": {
        "php": "^8.0",
        "symfony/console": "^5.3"
    },
    "require-dev": {
        "infection/infection": "^0.21",
        "phpunit/phpunit": "^9.5",
        "ramsey/uuid": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "GetWith\\CoffeeMachine\\Apps\\Machine\\Console\\": "apps/machine/console/src",
            "GetWith\\CoffeeMachine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GetWith\\CoffeeMachine\\Apps\\Machine\\Console\\Tests\\": "apps/machine/console/tests",
            "GetWith\\CoffeeMachine\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}