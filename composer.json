{
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "ext-zend-opcache": "*",
        "doctrine/dbal": "^3.1",
        "doctrine/orm": "^2.10",
        "ramsey/uuid": "^3.9",
        "symfony/cache": "^5.3",
        "symfony/config": "^5.3",
        "symfony/console": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "infection/infection": "^0.21",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Gelateria\\Apps\\Shop\\Console\\": "apps/shop/console/src",
            "Gelateria\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gelateria\\Apps\\Shop\\Console\\Tests\\": "apps/shop/console/tests",
            "Gelateria\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}